// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema
datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model categoryUsers {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  description String
}

model Users {
  id       String         @id @default(auto()) @map("_id") @db.ObjectId
  name     String
}
